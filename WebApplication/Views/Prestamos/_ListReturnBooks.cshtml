@model CommonComponents.DTOs.DetallePrestamosDTO

<div class="form-group">
    @Html.LabelFor(model => model.dtp_libro, "Libro", htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownList("dtp_libro", (IEnumerable<SelectListItem>)TempData["lista"], "Seleccionar libro", htmlAttributes: new { @class = "form-control" })
        @*@Html.DropDownListFor(x => x.dtp_libro, (IEnumerable<SelectListItem>)ViewBag.listLibros)*@
        @Html.ValidationMessageFor(model => model.dtp_libro, "", new { @class = "text-danger" })
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Guardar" class="btn btn-default" />
    </div>
</div>
