@model IEnumerable<DataAccess.Models.Usuarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.usu_nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usu_direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usu_telefono)
        </th>
