        <td>
            @Html.DisplayFor(modelItem => item.usu_nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usu_direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usu_telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usu_correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usu_estado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.usu_documento }) |
            @Html.ActionLink("Details", "Details", new { id=item.usu_documento }) |
